<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<virtualDatacenter>
    <link rel="location" type="application/vnd.abiquo.datacenter+xml; version=3.0" href="http://localhost/api/cloud/locations/<%= datacenterId %>"/>
    <link rel="enterprise" href="http://localhost/api/admin/enterprises/<%= enterpriseId %>"/>
    <hypervisorType><%= hypervisorType %></hypervisorType>
	<name><%= name %></name>
    <cpuHard>0</cpuHard>
    <cpuSoft>0</cpuSoft>
    <ramHard>0</ramHard>
    <ramSoft>0</ramSoft>
    <vlansHard>0</vlansHard>
    <vlansSoft>0</vlansSoft>
    <publicIpsHard>0</publicIpsHard>
    <publicIpsSoft>0</publicIpsSoft>
    <network>
        <name>default</name>
        <type>INTERNAL</type>
        <address>192.168.1.0</address>
        <mask>24</mask>
        <gateway>192.168.1.5</gateway>
        <ipv6>false</ipv6>
        <strict>false</strict>
    </network>
</virtualDatacenter>